            </MenuItem>
            <MenuItem x:Name="MenuItem_PanoramaWindow" Header="Панорама" Height="23" Click="MenuItem_PanoramaWindow_Click"/>
            <MenuItem x:Name="menuItem_Requisites" Header="Реквизиты клиники" Click="menuItem_Requisites_Click"/>
