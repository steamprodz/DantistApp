        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:custom="clr-namespace:DantistApp.Elements"
        xmlns:local="clr-namespace:DantistApp"
        xmlns:UserControls="clr-namespace:DantistApp.UserControls" mc:Ignorable="d" x:Class="DantistApp.MainWindow"
        Title="Dantist Application" Height="656.4" Width="1024" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">

    <Window.CommandBindings>
        <CommandBinding Command="Redo" Executed="commandBinding_Redo_Executed" />
        <CommandBinding Command="Undo" Executed="commandBinding_Undo_Executed" />
    </Window.CommandBindings>

    <Window.InputBindings>
        <KeyBinding Key="Z" Modifiers="Ctrl+Shift" Command="Redo" />
        <KeyBinding Key="Z" Modifiers="Ctrl" Command="Undo" />
    </Window.InputBindings>

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="26*"/>
            <RowDefinition Height="386*"/>
            <RowDefinition Height="70*"/>
            <RowDefinition Height="59*"/>
            <RowDefinition Height="53*"/>
            <RowDefinition Height="32*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="617*"/>
            <ColumnDefinition Width="401*"/>
        </Grid.ColumnDefinitions>
        <Canvas x:Name="CanvasMain" HorizontalAlignment="Left" Height="385" VerticalAlignment="Top" Width="612" Grid.Row="1" MouseLeftButtonDown="MainCanvas_MouseLeftButtonDown" MouseLeftButtonUp="MainCanvas_MouseLeftButtonUp" MouseMove="MainCanvas_MouseMove" Margin="0,0.6,0,0" MouseRightButtonDown="canvas_main_MouseRightButtonDown">
            <Canvas.Background>
                <ImageBrush ImageSource="Image/main.png" Stretch="Uniform"/>
            </Canvas.Background>
        </Canvas>

