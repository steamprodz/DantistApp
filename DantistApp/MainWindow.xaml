            <MenuItem Header="Файл">
                <MenuItem Header="Сохранить в PDF" HorizontalAlignment="Left" Width="178" Click="menuItem_SaveAsPdf_Click"/>
                <MenuItem x:Name="menuItem_Exit" Header="Выход" HorizontalAlignment="Left" Width="178" Click="menuItem_Exit_Click"/>
            </MenuItem>
            <MenuItem Header="Правка">
                <MenuItem Header="Отменить" HorizontalAlignment="Left" Width="320" InputGestureText="Ctrl+Z" Command="Undo"/>
                <MenuItem Header="Повторить" HorizontalAlignment="Left" Width="320" InputGestureText="Ctrl+Shift+Z" Command="Redo"/>
                <MenuItem Header="Удалить все объекты" HorizontalAlignment="Left" Width="320" Click="MenuItem_RemoveAllElements_Click"/>
                <MenuItem Header="Удалить выделенные объекты" HorizontalAlignment="Left" Width="320" Click="MenuItem_RemoveSelectedElements_Click"/>
            </MenuItem>
            <MenuItem Header="Зубы" Height="23">
                <MenuItem x:Name="menuItem_MergeAllTeeth" Header="Объеденить все" Click="menuItem_MergeAllTeeth_Click"/>
                <MenuItem x:Name="menuItem_UnmergeAllTeeth" Header="Разъединить все" Click="menuItem_UnmergeAllTeeth_Click"/>
